{
  "name": "sensu-sensugo-datasource",
  "version": "1.0.2",
  "description": "Sensu Go Data Source for Grafana",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack.config.prod.js",
    "dev": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "precommit": "lint-staged yarn",
    "pretty": "prettier --write src/**/*.ts"
  },
  "author": "Novatec Consulting GmbH",
  "license": "",
  "lint-staged": {
    "*.{js,ts,json,css,md}": ["prettier --write", "git add"]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "useTabs": false,
    "printWidth": 90,
    "bracketSpacing": false,
    "semi": true
  },
  "dependencies": {
    "lodash": "^4.17.19"
  },
  "devDependencies": {
    "@types/grafana": "github:CorpGlory/types-grafana.git",
    "@types/lodash": "^4.14.74",
    "@types/papaparse": "^4.5.7",
    "@types/react": "^16.6.3",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "2.1.0",
    "grafana-sdk-mocks": "github:grafana/grafana-sdk-mocks",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.0",
    "ng-annotate-webpack-plugin": "^0.3.0",
    "prettier": "1.10.2",
    "release-it": "^10.0.3",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "style-loader": "^0.21.0",
    "ts-loader": "^4.3.0",
    "typescript": "^2.8.3",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "4.28.4",
    "webpack-cli": "3.2.1"
  },
  "resolutions": {
    "webpack/watchpack/chokidar/fsevents": "1.2.4"
  }
}
